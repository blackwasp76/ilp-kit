{
  "name": "ilp-kit",
  "version": "1.0.0",
  "description": "ILP kit",
  "main": "bin/server.js",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/interledgerjs/ilp-kit"
  },
  "author": "Interledger Team <info@interledger.org>",
  "homepage": "https://github.com/interledgerjs/ilp-kit",
  "keywords": [
    "five-bells",
    "ilp",
    "ledger",
    "wallet",
    "ui",
    "payments",
    "finance"
  ],
  "scripts": {
    "start": "DEBUG_COLORS=1 concurrently -p \"[{name}]\" -n \"ledger,connector,api,wallet,localtunnel\" -c \"red,magenta,blue,cyan,green\" \"npm run start-prod-ledger\" \"npm run start-prod-connector\" \"npm run start-prod-api\" \"npm run start-prod\" \"npm run start-localtunnel\"",
    "start-prod": "better-npm-run start-prod",
    "start-prod-api": "better-npm-run start-prod-api",
    "start-prod-ledger": "better-npm-run start-prod-ledger",
    "start-prod-connector": "better-npm-run start-prod-connector",
    "start-localtunnel": "better-npm-run start-localtunnel",
    "build": "touch src/theme/variables-custom.scss ; better-npm-run build",
    "build-dlls": "touch src/theme/variables-custom.scss ; node ./bin/dlls.js",
    "postinstall": "npm rebuild node-sass",
    "lint": "eslint -c .eslintrc src api",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run start-dev-api",
    "start-dev-ledger": "better-npm-run start-dev-ledger",
    "start-dev-connector": "better-npm-run start-dev-connector",
    "watch-client": "better-npm-run watch-client",
    "dev": "DEBUG_COLORS=1 concurrently -p \"[{name}]\" -n \"watcher,ledger,connector,api,wallet\" -c \"yellow,red,magenta,blue,cyan\" \"npm run watch-client\" \"npm run start-dev-ledger\" \"npm run start-dev-connector\" \"npm run start-dev-api\" \"npm run start-dev\"",
    "test": "touch src/theme/variables-custom.scss ; karma start",
    "test-api": "better-npm-run test-api",
    "apidoc": "apidoc -i api/src/controllers/ -o apidoc-out",
    "key": "./node_modules/ilp-kit-cli/bin/configure.js key",
    "configure": "./node_modules/ilp-kit-cli/bin/configure.js configure",
    "peer": "./node_modules/ilp-kit-cli/bin/configure.js peer",
    "depeer": "./node_modules/ilp-kit-cli/bin/configure.js depeer",
    "list-peers": "./node_modules/ilp-kit-cli/bin/configure.js list"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "start-prod-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production"
      }
    },
    "start-prod-ledger": {
      "command": "node ./bin/ledger.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-prod-connector": {
      "command": "node ./bin/connector.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-localtunnel": {
      "command": "node ./bin/localTunnel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development"
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development"
      }
    },
    "start-dev-ledger": {
      "command": "node ./bin/ledger.js | bunyan --color",
      "env": {
        "NODE_PATH": "./node_modules/five-bells-ledger",
        "NODE_ENV": "development"
      }
    },
    "start-dev-connector": {
      "command": "node ./bin/connector.js | bunyan --color",
      "env": {
        "NODE_PATH": "./node_modules/ilp-connector",
        "NODE_ENV": "development"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --progress --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test-api": {
      "command": "node ./bin/testApi.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
